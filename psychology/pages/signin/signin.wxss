/* pages/signin/signin.wxss */
.signin_form {
  position: absolute;
  top: 60rpx;
  left: 50%;
  transform: translateX(-50%);
  box-sizing: border-box;
  padding: 32rpx;
  width: 94%;
  /* min-height: 520rpx; */
  background-color: #fff;
  box-shadow: 0px 0px 29rpx 0px rgba(109, 109, 109, 0.35);
  border-radius: 20rpx;
}

.skip_class {
  display: flex;
  justify-content: flex-end;
  color: #999;
}