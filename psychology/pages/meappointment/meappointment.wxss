.favorite-content {
  margin: 12rpx 0;
  background-color: #fff !important;
}

/* 右滑删除的样式 */
.van-swipe-cell__left,
.van-swipe-cell__right {
  background-color: #f44;
  height: inherit;
  width: 65px;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}

.news-warp {
  padding: 16rpx 32rpx;
}

.item-cate {
  margin-bottom: 20rpx;
  padding: 12rpx;
  box-sizing: border-box;
  border-radius: 12rpx;
  background-color: #fff;
}

.dish-name {
  text-align: center;
}

.active_warp {
  color: #969799;
  font-size: 26rpx;
  padding: 20rpx 32rpx;
  background-color: #fff;
  line-height: 44rpx;
}